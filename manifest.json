{
  "name": "Plato Cat",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A cat extension",
  "homepage_url": "https://platocat.saturdaycode.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src chrome://favicon https://*.tenor.com data: https://*.thao.pw;",
  "permissions": [
    "topSites",
    "chrome://favicon/",
    "storage",
    "unlimitedStorage",
    "https://www.google-analytics.com/"
  ],
  "background": {
    "scripts": [
      "js/lodash.min.js",
      "js/google-analytics-bundle.js",
      "js/background.js"
    ],
    "persistent": false
  }
}